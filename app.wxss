/**app.wxss**/
@import "common/common.wxss";
@import "common/util.wxss";
@import "common/flex.wxss";

view {
  box-sizing: border-box;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

/* 按钮的 外部样式reset*/
.share-btn {
  border: none !important;
  padding: 0;
	margin: 0;
	background-color: transparent;
}

.share-btn::after{
  border: none;
  content: unset;
}